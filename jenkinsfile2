node 
{
   def mvnHome
     checkout scm;
   stage('checkout') 
   { 
   
   def url =readProperties  file: 'propertiesfile'
   echo "${url}"
   def Var1= url.APP_GIT_URL
   echo "Var1=${Var1}"
   git "${Var1}"
   mvnHome = tool 'Maven'
   }
   stage('Build')
   {
     checkout scm;
   def url =readProperties  file: 'propertiesfile'
   echo "${url}"
   def Var2 = url.APP_BUILD
   echo "Var2=${Var2}"
   sh "'${mvnHome}/bin/mvn' -Dmaven.test.failure.ignore clean package"
   }
 
}
