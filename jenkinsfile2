node 
{
   def mvnHome
     checkout scm;
     
   def url =readProperties  file: 'propertiesfile'
   stage('checkout') 
   { 
   
   echo "${url}"
   def Var1= url.APP_GIT_URL
   echo "Var1=${Var1}"
   git "${Var1}"
   mvnHome = tool 'MAVEN_HOME'
   }
   stage('Build')
   {
   def Var2= url.APP_BUILD
   echo "${Var2}"
     sh "${Var2}"
   }
 
}
