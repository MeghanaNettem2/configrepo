node 
{
   stage('checkout') 
   { 
   def url
   url =load "configrepo/jenkinsfile"
   git $(url)

   }
   stage('Build')
   {
          sh "mvn clean install" 
   }
stage('SonarQube analysis') 
    { 
         sh 'mvn sonar:sonar -Dsonar.host.url=http://my40867dns.eastus2.cloudapp.azure.com' 
     }
 
}
